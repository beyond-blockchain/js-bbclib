    "@babel/preset-env": "7.7.7",
    "@babel/register": "7.7.7",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "can-npm-publish": "1.3.2",
